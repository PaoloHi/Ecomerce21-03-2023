<script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>

<script>
    var strValue = "@((string)ViewBag.mssg)";

    if (strValue !== null && strValue !== "") {

        if (strValue == "Error") {
            Swal.fire(
                'Error',
                'El correo introducido ha sido registrado previamente',
                'warning'
            )

        } else {

            Swal.fire(
                'Registro exitoso',
                strValue,
                'success'
            )
        }


    }

</script>



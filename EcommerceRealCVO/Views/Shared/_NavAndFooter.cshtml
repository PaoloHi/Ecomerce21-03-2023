<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, user-scalable=1.0">
     <title>@ViewData["Title"]</title>
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v6.1.1/css/all.css"
          integrity="sha384-/frq1SRXYH/bSyou/HUp/hib7RVN1TawQYja658FEOodR/FQBKVqT9Ol+Oz3Olq5" crossorigin="anonymous">
    <link rel="stylesheet" href="~/css/styles.css" />
    <link rel="stylesheet" href="~/css/StyleMainPropiedades.css">
    <link href="~/css/sweetalert2.css" rel="stylesheet" />
    <link rel="icon" type="image/png" sizes="16x16" href="~/img/imagen-logo/logo_oficial.png" />




    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.min.js" integrity="sha384-IDwe1+LCz02ROU9k972gdyvl+AESN10+x7tBKgc9I5HFtuNz0wWnPclzo6p9vxnk" crossorigin="anonymous"></script>
    <link rel="icon" type="image/png" sizes="16x16" href="img/logo_oficial.png">
</head>
<body>
    <!--AQUI COMIENZA EL NAVBAR-->
    <header class="header sticky-top">
        <nav class="navbar navbar-expand-lg navbar-dark p-2 sticky-top activar">
            <div class="container-fluid">
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                        data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav col-lg-6 ">
                           <li class="nav-item">
                            <a class="nav-link" asp-controller="Home" asp-action="Index">Inicio</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-controller="Ecommerce" asp-action="Propiedades">Propiedades</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-controller="Ecommerce" asp-action="PlanesEcommerce">Planes</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#">Nosotros</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" >Ayuda</a>
                        </li>
                    </ul>

                    <ul class="navbar-nav col-lg-4 d-lg-block">
                        <li class="nav-item">
                             <a class="" href="">
                                <img src="~/img/imagenes__e-commerce/logo_oficial_blanco.png" class="img-fluid image-size" loading="lazy" alt="">
                            </a>
                        </li>

                    </ul>

                    <ul class="navbar-nav col-lg-2">
                          <li class="nav-item">

                            <a class="nav-link rounded me-lg-3 mt-1 text-start d-flex align-items-center"
                              asp-controller="Ecommerce" asp-action="PlanesEcommerce"
                               style="background-color:#ff7f40; border-radius: 5px!important; height: 30px;">

                                <h6 class="text-light mt-2">Afiliarme</h6>
                            </a>


                        </li>
                        <li class="nav-item">

                            <a class="nav-link rounded ml-2 " asp-controller="Ecommerce" asp-action="Contacto">
                                <i class="fa-solid fa-phone size-icon-navbar"></i>
                            </a>


                        </li>
                        <li class="nav-item ">

                            <a class="nav-link rounded ml-2 " asp-controller="Cuentas" asp-action="Acceso">
                                <i class="fa-solid fa-user size-icon-navbar"></i>
                            </a>

                        </li>
                       

                    </ul>



                </div>
            </div>
        </nav>
    </header>
    <!--Aqui termina mi header-->
    
        @RenderBody()
   

    <!--AQUI VA EL FOOTER-->


    


        <!--Aqui comienza el footer-->
    <footer class="text-white text-center text-lg-start main_backg pb-lg-3 pb-md-3">
            <!-- Grid container -->
            <div class="container">
                <!--Grid row-->
                <div class="row ">
                    <!--Grid column-->
                <div class="col-lg-4 col-md-6   mb-md-0 mb-4 d-grid align-items-end b mt-lg-2">




                    <div class="d-inline-flex mt-lgl2 align-items-center align-text-bottom">
                        <i class="fas fa-envelope size-icons-footer-2 mt-2"></i>
                            <h6 class="ms-2 size-text-footer mt-3">contacto@hotmail.com</h6> 
                            </div>

                    <div class="d-inline-flex">
                        <i class="fas fa-phone size-icons-footer-2 mt-1"></i>
                        <h6 class="ms-2 size-text-footer">55 4532 4598</h6>
                            </div>
                      

                    </div>

                    <!--Grid column-->
                    <div class="col-lg-3 col-md-12  mb-md-0 ms-lg-4 d-grid align-items-end mt-3 justify-content-center">

                        <div class="mb-2 mt-2 ">
                            
                            <!-- Facebook -->
                        <a type="button" class="me-4 icon"><i class=" fab icon-background fa-facebook-f size-icons-footer "></i></a>
                                                   
                            <!-- Instagram -->
                        <a type="button" class="me-4 icon"><i class="fab icon-background fa-instagram size-icons-footer"></i></a>
                            <!-- Whatsapp -->
                        <a type="button" class="icon"><i class="fa-brands fa-whatsapp icon-background size-icons-footer me-4"></i></a>
                            <!-- Linkedin -->
                        <a type="button" class="icon"><i class="fa-brands fa-linkedin-in icon-background size-icons-footer"></i></a>
                        </div>
                       
                        <div class=" mx-auto">
                            <a href="" class="size-text-footer-copyr">&copy;2022 Corporativo Valle Oriente</a>
                        </div>

                    </div>
                    <!--Grid column-->
                <div class="col-lg-4 col-md-12 mb-4 mb-lg-2 mb-md-0 ms-lg-5 d-grid align-items-end justify-content-end mt-2">

                        <div class="">
                        <a href="" class="size-text-footer justify-content-end d-grid">Aviso de privacidad</a>
                            
                            <a href="" class="color-link-footer size-text-footer">Términos y condiciones</a>
                            

                        </div>

                   
                    </div>
                   
                    
               
                </div>
                <!--Grid row-->
            </div>

            

        </footer>
        
        <!--AQUI TERMINO EL FOOTER-->
    <!--Scripts-->

     <script src="~/js/sweetalert2.js"></script>
    @section Scripts{
    <script>
        var strValue = "@((string)ViewBag.mssg)";
        var strValueE = "@((string)ViewBag.mssgE)";


        if (strValue !== null && strValue !== "") {

            Swal.fire(
                'Registro exitoso',
                strValue,
                'success'
            )

        }

        if (strValueE !== null && strValueE !== "") {

            Swal.fire(
                'Error',
                strValueE,
                'error'
            )

        }
    </script>
    }

    <!--Scripts-->
</body>
</html>